{
    "manifest_version": 3,
    "name": "SoundCloud Discord RPC Extension",
    "version": "1.44.6",
/*    "permissions": [
        "http://localhost/*"
    ],
*/

    "action": {
        "default_icon": {
            "16": "/images/get_started16.png",
            "32": "/images/get_started32.png",
            "48": "/images/get_started48.png",
            "128": "/images/get_started128.png"
        }
    },

    "icons": {
        "16": "/images/get_started16.png",
        "32": "/images/get_started32.png",
        "48": "/images/get_started48.png",
        "128": "/images/get_started128.png"
    },

    "content_scripts": [{
        "matches": ["<all_urls>"],
        "js": ["script.js"]
    }]
}